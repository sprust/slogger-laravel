{
    "name": "slogger/laravel",
    "description": "SLogger for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel",
            "email": "sppetrovich@mail.ru"
        }
    ],
    "require": {
        "ext-pcntl": "*",
        "ext-posix": "*",
        "laravel/framework": "^9.0 | ^10.0 | ^11.0 | ^12.0",
        "symfony/process": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "SLoggerLaravel\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "SLoggerLaravel\\Tests\\": "tests/"
      }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SLoggerLaravel\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^10.9",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5"
    }
}
